<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from DashBoardContaDAOService WSDL service -->

  <service name="DashBoardContaDAO" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://dao"/>
      <parameter name="wsdlServiceElement" value="DashBoardContaDAOService"/>
      <parameter name="schemaQualified" value="http://modelo,http://dao"/>
      <parameter name="wsdlServicePort" value="DashBoardContaDAO"/>
      <parameter name="className" value="dao.DashBoardContaDAOSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="DashBoardContaDAO"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="buscarContaUsuario" qname="operNS:buscarContaUsuario" xmlns:operNS="http://dao" returnQName="retNS:buscarContaUsuarioReturn" xmlns:retNS="http://dao" returnType="rtns:Conta" xmlns:rtns="http://modelo" soapAction="" >
        <parameter qname="pns:idusuario" xmlns:pns="http://dao" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="buscarContaUsuario"/>

      <typeMapping
        xmlns:ns="http://modelo"
        qname="ns:Conta"
        type="java:modelo.Conta"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
